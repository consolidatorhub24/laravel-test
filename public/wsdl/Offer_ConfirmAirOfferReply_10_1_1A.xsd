<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/POFRKR_10_1_1A" xmlns="http://xml.amadeus.com/POFRKR_10_1_1A" elementFormDefault="qualified">
  <xs:element name="Offer_ConfirmAirOfferReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">Confirm Offer</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="soldItinerary" minOccurs="0" maxOccurs="12">
          <xs:annotation>
            <xs:documentation xml:lang="en">Itinerary that has been confirmed and firmly booked</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="originDestination" type="OriginAndDestinationDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Origin and Destination of a set of segments</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="segmentDetails" minOccurs="0" maxOccurs="12">
                <xs:annotation>
                  <xs:documentation xml:lang="en">all segment details constituing a given ODI</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="flightDetails" type="TravelProductInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Provides the full flight details of the segment that have been sold</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="originalStatusCode" minOccurs="0" type="RelatedProductInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Transport the status code and the NIP</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="tstList" minOccurs="0" maxOccurs="198">
          <xs:annotation>
            <xs:documentation xml:lang="en">List of TST tattoos created. As we can have 99 passenger per PNR, and a TST split with the Infant , the max number of TST is 198.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="uniqueReference" type="ItemReferencesAndVersionsType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">TST tattoo number created by the transaction.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="paxReference" minOccurs="0" type="ReferenceInformationType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Tatoos of the PAX associated to created TST</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="offerConfirmationResponse" type="OfferInformationType">
          <xs:annotation>
            <xs:documentation xml:lang="en">provides the outcome / status of the confirmation for each offer</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
